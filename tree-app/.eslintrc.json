{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": 
    [
    "eslint:recommended",
    "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types": "warn"
    }
}
